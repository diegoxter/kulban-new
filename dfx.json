{
  "canisters": {
    "kulban_backend": {
      "build": "npx azle kulban_backend",
      "candid": "src/kulban_backend/kulban_backend.did",
      "declarations": {
        "node_compatibility": true
      },
      "gzip": true,
      "main": "src/kulban_backend/src/index.ts",
      "tech_stack": {
        "cdk": {
          "azle": {}
        },
        "language": {
          "javascript": {},
          "typescript": {}
        }
      },
      "type": "custom",
      "wasm": ".azle/kulban_backend/kulban_backend.wasm"
    },
    "kulban_frontend": {
      "dependencies": [
        "kulban_backend"
      ],
      "source": [
        "src/kulban_frontend/dist"
      ],
      "type": "assets",
      "workspace": "kulban_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}